{
  "all": true,
  "check-coverage": true,
  "reporter": ["html", "text", "lcov"],
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 70,
  "exclude": [
    "test/**",
    "node_modules/**",
    "dist/**",
    ".eslintrc.js"
  ],
  "temp-dir": ".nyc_output",
  "report-dir": "coverage"
}
